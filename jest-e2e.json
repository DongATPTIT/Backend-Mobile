{
    "moduleNameMapper": {
        "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "rootDir": "../",
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    }
}